<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="isa.mvp.tarena.isax.activity.MainFragmentActivity"
    tools:showIn="@layout/app_bar_main_fragment">

    <LinearLayout
        android:id="@+id/fragmentRoot"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/botmenu"
        android:orientation="vertical" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_above="@+id/botmenu"
            android:background="#929292" />

        <include
            android:id="@+id/botmenu"
            layout="@layout/include_bot_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"></include>

</RelativeLayout>
